{
  "name": "signaling-server",
  "version": "1.0.3",
  "author": "Unrealos",
  "license": "MIT",
  "description": "The signaling-server npm module is designed to simplify the process of exchanging data between a web client (Metaeditor) and the Unreal Engine.",
  "public": true,
  "homepage": "https://metaeditor.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markolofsen/metaeditor.git"
  },
  "bugs": {
    "url": "https://github.com/markolofsen/metaeditor/issues"
  },
  "keywords": [
    "node",
    "typescript",
    "pixel streaming",
    "unreal engine",
    "epic games",
    "signaling server"
  ],
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "node ./lib/src/index.js",
    "lint": "eslint lib",
    "lint:fix": "eslint lib --fix",
    "test": "jest --test-path-pattern=lib/test --watch-all=true --coverage=true",
    "test:ppo": "jest --test-path-pattern=lib/test",
    "build": "rm -rf dist && mkdir -p dist && babel lib --out-dir dist",
    "examples": "cd ./dist/examples && node run.js",
    "prepublishOnly2": "npm run lint && npm run build && npm run test:ppo"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "express-session": "^1.15.6",
    "helmet": "^3.21.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "run-script-os": "^1.1.6",
    "ws": "^7.1.2",
    "y18n": "^5.0.5",
    "yargs": "^15.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/preset-env": "^7.9.6",
    "babel-jest": "^26.0.1",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^26.0.1"
  },
  "keywords": [
    "es",
    "module",
    "npm",
    "template"
  ]
}